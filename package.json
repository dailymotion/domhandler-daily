{
  "name": "domhandler-daily",
  "version": "2.3.0",
  "description": "handler for htmlparser2 that turns pages into a dom",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha -R list && jshint index.js test/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dailymotion/DomHandler-daily.git"
  },
  "keywords": [
    "dom",
    "htmlparser2"
  ],
  "dependencies": {
    "domelementtype": "1"
  },
  "devDependencies": {
    "htmlparser2": "3.8",
    "mocha": "1",
    "jshint": "~2.3.0"
  },
  "author": "Romain Berger <romain.berger@dailymotion.com>",
  "jshintConfig": {
    "quotmark": "double",
    "trailing": true,
    "unused": true,
    "undef": true,
    "node": true,
    "proto": true,
    "globals": {
      "it": true
    }
  }
}
